{
  "name": "frontend",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "SkillTree Dashboard UI",
  "author": "SkillTree Team",
  "scripts": {
    "licenseDepCheck": "license-checker --production --onlyAllow='MIT;Apache-2.0' --summary",
    "licenseHeaderCheck": "license-check-and-add check -f '../license-add/license-add-config.json'",
    "licenseHeaderAdd": "license-check-and-add add -f ../license-add/license-add-config.json",
    "licenseCheck": "npm run licenseHeaderCheck && npm run licenseDepCheck",
    "serve": "vue-cli-service serve",
    "build": "npm run licenseCheck && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "apexcharts": "3.26.0",
    "axios": "0.21.1",
    "bootstrap": "4.6.0",
    "bootstrap-vue": "2.21.2",
    "core-js": "3.9.1",
    "moment": "2.29.1",
    "numeral": "2.0.6",
    "vue": "2.6.12",
    "vue-apexcharts": "1.6.0",
    "vue-router": "3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.12",
    "@vue/cli-plugin-eslint": "4.5.12",
    "@vue/cli-service": "4.5.12",
    "babel-eslint": "10.1.0",
    "eslint": "7.22.0",
    "eslint-plugin-vue": "7.7.0",
    "license-check-and-add": "4.0.2",
    "license-checker": "25.0.1",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
